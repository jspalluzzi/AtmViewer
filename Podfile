# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target ‘AtmViewer’ do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for AtmViewer
pod 'Alamofire', '~> 4.0'
pod 'SwiftyJSON'
pod 'GoogleMaps'
pod 'GooglePlaces'

  target 'AtmViewerTests' do
    inherit! :search_paths
    # Pods for testing
pod 'Alamofire', '~> 4.0'
pod 'SwiftyJSON'
pod 'GoogleMaps'
pod 'GooglePlaces'
  end

  target 'AtmViewerUITests' do
    inherit! :search_paths
    # Pods for testing
pod 'Alamofire', '~> 4.0'
pod 'SwiftyJSON'
pod 'GoogleMaps'
pod 'GooglePlaces'
  end

end
